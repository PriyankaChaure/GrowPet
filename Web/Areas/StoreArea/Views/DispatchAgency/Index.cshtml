@model IEnumerable<DispatchAgency>

<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
           
            <h4 class="card-title">Category List</h4>
            <a class="btn btn-gradient-primary btn-rounded btn-fw" asp-action="Create" asp-controller="DispatchAgency"> Create </a>
            <div class="table-responsive">
            <table class="table table-hover">
                <thead>
                    <tr>
                        
                        <th> #</th>
                        <th>Agency Name</th>
                        <th>Address</th>
                        <th> Email Id</th>
                        <th>Mobile No</th>
                        <th>ContactPerson</th>
                        <th> Actions</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        int srno = 1;
                    }
                    @foreach (var temp in Model)
                    {
                    <tr>
                       
                        <td>@srno</td>
                        <td>@temp.FullName</td>
                         <td>@temp.Address</td>
                         <td>@temp.EmailId</td>
                         <td>@temp.MobileNo</td>
                         <td>@temp.ContactPerson</td>
                        <td>
                             <a class="btn btn-gradient-info btn-rounded btn-fw" asp-action="Edit" asp-route-id="@temp.DispatchAgencyId">Edit</a> 
                             <a class="btn btn-gradient-danger btn-rounded btn-fw" asp-action="Delete" asp-route-id="@temp.DispatchAgencyId">Delete</a>
                        </td>
 
                    </tr>
                        srno++;
                    }
                </tbody>
            </table>
            </div>
        </div>
    </div>
</div>
