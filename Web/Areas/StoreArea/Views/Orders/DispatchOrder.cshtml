@model DispatchOrder
<script src="~/jquery/jquery.js"></script>
<div class="card">
    <div class="card-body">
        <h4 class="card-title">Create  </h4>

        <div>
            @if (ViewBag.Message != null)
            {
                <div class="alert alert-info">@ViewBag.Message</div>
            }
        </div>

        <form class="forms-sample" enctype="multipart/form-data" asp-action="DispatchOrder" method="post">
          
            <div class="form-group">
                <label class="form-label">  Order Id:@TempData["OrderId"]</label>
            </div>
           
            <div class="form-group">
                <label class="form-label">Expected Reach Date</label>
                <input asp-for="ExpectedReachDate" class="form-control" placeholder="ExpectedReachdate">
                <span asp-validation-for="ExpectedReachDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="form-label"> DocketNo</label>
                <textarea asp-for="DocketNo" class="form-control" placeholder="DocketNo"></textarea>
                <span asp-validation-for="DocketNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="form-label">  Dispatch Agency Name</label>
                <select class="form-select" id="Country" asp-for="DispatchAgencyId" asp-items="@ViewBag.DispatchAgencyId">
                    <option value="0">Select Agency</option>
                </select>
                <span asp-validation-for="DispatchAgencyId" class="text-danger"></span>
            </div>
           
            <button type="submit" asp-route-id="@TempData["OrderId"]" class="btn btn-gradient-primary me-2">Save</button>
            <button class="btn btn-light"><a asp-action="Index" asp-controller="Orders">Cancel</a></button>
        </form>
    </div>
</div>
